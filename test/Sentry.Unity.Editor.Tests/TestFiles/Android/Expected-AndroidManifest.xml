<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="io.sentry.unity.android.test">

  <application
      android:name=".MyUnityTestApplication"
      android:allowBackup="false"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:theme="@style/AppTheme"
      tools:ignore="GoogleAppIndexingWarning, UnusedAttribute">
    <meta-data android:name="io.sentry.dsn" android:value="https://1@1.ingest.sentry.io/1" />
    <meta-data android:name="io.sentry.debug" android:value="true" />
    <meta-data android:name="io.sentry.debug.level" android:value="debug" />
    <!--    <meta-data android:name="io.sentry.ndk.scope-sync.enable" android:value="true" />-->
    <meta-data android:name="io.sentry.release" android:value="app@1+2" />
    <meta-data android:name="io.sentry.auto-session-tracking.enable" android:value="false" />
    <meta-data android:name="io.sentry.environment" android:value="env" />
    <meta-data android:name="io.sentry.sample-rate" android:value="0.5" />
  </application>
</manifest>
